  {
    "type": "Feature",
    "geometry": {
      "type": "Point",
      "coordinates": [
        -166.18666,
        68.69446
      ]
    },
    "properties": {
      "observer": "Jack Lane",
      "observation": "North side has ice floating, south side floating away from beach. Ice leaving, no more piled up on the beaches. Temperature around 49f, people going up the coast to work on cabins and pick eggs. Using the remaining ice not he beach for ice cellars and to keep fresh eggs fresh.",
      "obs_date": "2017-07-31",
      "multimedia": [
        {
          "fullsize_url": "/static/aaokh/31-07-2017-1.png"
        }
      ]
    }
  },
  {
